package chapter13.lambda;

public class App {
	
	public static void main(String[] args) {
		Human tom = new Human();
		tom.walk();
		walking(tom);
		
		Robot sam = new Robot();
		sam.walk();
		walking(sam);
		
		walking(new Walkable() {
			
			@Override
			public void walk() {
				System.out.println("이름 없는 클래스 객체가 걷는다.");
			}
		});
	}
	
	public static void walking(Walkable any) {
		any.walk();
	}

}
