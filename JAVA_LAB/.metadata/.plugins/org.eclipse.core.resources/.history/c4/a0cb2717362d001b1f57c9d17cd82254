package chapter7;

public class TwoDimension {

	public static void main(String[] args) {
		// 2차원 배열을 만들어 보자
		int[][] arr = { {1,2,3}, {4,5,6}};
		
		for(int i=0; i<arr.length; i++) {
			
			for(int j=0; j<arr[i].length; j++) {
				System.out.print(arr[i][j]);
			}
			System.out.println();
		}
	}

}
