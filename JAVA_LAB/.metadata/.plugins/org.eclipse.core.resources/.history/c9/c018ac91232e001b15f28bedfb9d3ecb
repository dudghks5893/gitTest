package ArrayList;

public class ex3 {

	public static void main(String[] args) {
		int[] A = new int[5];
		int nCnt = 0;
		for(int i=1; i < 11; i++) {
			if((i % 2) == 0) {
				A[nCnt] = i;
				++nCnt;
			}
		}
		int sum = 0;
		for(int i=0; i<A.length; i++) {
			sum += A[i];
		}
		System.out.println(sum);
	}

}
