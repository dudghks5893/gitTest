package testEx.solo;

public class Methodex6 {

	public static void main(String[] args) {
		wear(3000,2000);
		wear(1000,2000);
	}
	public static void wear(int hp, int mp) {
		if(hp>=2000 && mp>=2000) {
			System.out.println("아이템 장착 완료!");
		}
		else {
			System.out.println("아이템을 착용할 수 없습니다.");
		}
	}
}


